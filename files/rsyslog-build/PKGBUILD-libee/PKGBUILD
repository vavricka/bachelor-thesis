# Maintainer: David Vavricka <vavricka.d@gmail.com>
pkgbase=libee
pkgname=${pkgbase}
pkgver=0.4.1
pkgrel=1
arch=('armv7h' 'armv7sp')
license='custom'

build() {
	cd ${startdir}/${pkgbase}-${pkgver}
	./configure --prefix=/mnt/hdd_1
	make V=1
}

package() {
	cd ${startdir}/${pkgname}-${pkgver}
	make install DESTDIR=${pkgdir}
}
